{
  "name": "conversational-agent-testing",
  "version": "1.0.0",
  "description": "Toolkit for evaluating and testing conversational AI agents (voice & chat) across multiple providers",
  "main": "dist/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Banana-Script/conversational-agent-testing.git"
  },
  "bugs": {
    "url": "https://github.com/Banana-Script/conversational-agent-testing/issues"
  },
  "homepage": "https://github.com/Banana-Script/conversational-agent-testing#readme",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "simulate": "tsx src/index.ts simulate",
    "create": "tsx src/index.ts create",
    "run": "tsx src/index.ts run",
    "list": "tsx src/index.ts list",
    "report": "tsx src/index.ts report",
    "download-agent": "tsx src/index.ts download-agent",
    "download": "tsx src/index.ts download-agent",
    "copy-agent": "tsx src/index.ts copy-agent",
    "generate:tests": "node scripts/generate-tests-base.js",
    "generate:tests:qa": "node scripts/generate-tests-qa.js",
    "check:claude-cli": "node scripts/check-claude-cli.js",
    "test": "echo \"Error: no test runner\" && exit 1"
  },
  "keywords": [
    "conversational-ai",
    "testing",
    "ai-agents",
    "voice-agents",
    "chat-agents",
    "elevenlabs",
    "testing-framework",
    "test-automation",
    "ai-testing"
  ],
  "author": "Banana-Script",
  "license": "MIT",
  "dependencies": {
    "@vapi-ai/server-sdk": "^0.11.0",
    "axios": "^1.6.7",
    "axios-retry": "^4.5.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.1",
    "ora": "^8.0.1",
    "yaml": "^2.3.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
